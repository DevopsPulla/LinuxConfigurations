#%Module -*- tcl -*-
##
## dot modulefile
##
proc ModulesHelp { } {
  global openmpiversion

  puts stderr "\tAdds openmpi-1.2.6 with intel-15.0.0.090 to your environment variables,"
}

module-whatis "adds openmpi-1.2.6 with intel-15.0.0.090 to your environment variables"



prepend-path      PATH                 /opt/apps/openmpi-1.2.6/intel-11.1/bin


prepend-path      LD_LIBRARY_PATH      /opt/apps/openmpi-1.2.6/intel-11.1/lib


prepend-path      LIBRARY_PATH        /opt/apps/openmpi-1.2.6/intel-11.1/lib


setenv           MPI_HOME          /opt/apps/openmpi-1.2.6/intel-11.1
setenv           MPI_RUN           /opt/apps/openmpi-1.2.6/intel-11.1/bin/mpirun

prepend-path      PATH                 /opt/apps/intel/Compiler/11.1/080/bin/intel64


prepend-path      LD_LIBRARY_PATH      /opt/apps/intel/Compiler/11.1/080/lib/intel64


prepend-path      LIBRARY_PATH         /opt/apps/intel/Compiler/11.1/080/lib/intel64

