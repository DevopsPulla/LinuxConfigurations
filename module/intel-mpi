#%Module -*- tcl -*-
##
## intel-cluster-runtime modulefile
##
proc ModulesHelp { } {
  puts stderr "\tAdds intel-cluster-runtime to your environment variables,"
}

module-whatis "Adds the Intel cluster runtime 64 bits to your environment variables"


prepend-path      PATH               /opt/apps/ansys_inc/v161/commonfiles/MPI/Intel/4.1.3/linx64/bin
prepend-path      LD_LIBRARY_PATH    /opt/apps/ansys_inc/v161/commonfiles/MPI/Intel/4.1.3/linx64/lib
prepend-path      LIBRARY_PATH       /opt/apps/ansys_inc/v161/commonfiles/MPI/Intel/4.1.3/linx64/lib



