#%Module -*- tcl -*-
##
## dot modulefile
##
proc ModulesHelp { } {
  global openmpiversion

  puts stderr "\tAdds openmpi-1.2.6 with intel-15.0.0.090 to your environment variables,"
}

module-whatis "adds openmpi-1.2.6 with intel-15.0.0.090 to your environment variables"



prepend-path      PATH                 /opt/apps/openmpi-1.6.5-bin/bin


prepend-path      LD_LIBRARY_PATH      /opt/apps/openmpi-1.6.5-bin/lib




setenv           MPI_HOME          /opt/apps/openmpi-1.6.5-bin
setenv           MPI_RUN           /opt/apps/openmpi-1.6.5-bin/bin/mpirun


