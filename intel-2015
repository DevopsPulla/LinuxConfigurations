#%Module -*- tcl -*-
##
## dot modulefile
##
proc ModulesHelp { } {
  global openmpiversion

  puts stderr "\tAdds intel-15.0.0.090 to your environment variables,"
}

module-whatis "adds intel-15.0.0.090 to your environment variables"


set               root                 /opt/apps/intel15/composer_xe_2015.2.164/ 

prepend-path      PATH                 $root/bin/intel64


prepend-path      LD_LIBRARY_PATH      $root/compiler/lib/intel64




